{
  "name": "dithermark-vue-color",
  "sideEffects": [
    "./src/components/*.vue",
    "./src/components/common/*.vue"
  ],
  "version": "1.1.0",
  "description": "Colorpicker for Dithermark app",
  "keywords": [
    "color",
    "vuejs"
  ],
  "main": "src/index.js",
  "scripts": {
    "dev": "webpack --progress --hide-modules --config  ./build/webpack.release.js",
    "release": "webpack --progress --hide-modules --config  ./build/webpack.release.min.js",
    "release:module": "webpack --progress --hide-modules --config  ./build/webpack.release.module.js && node ./build/convert-commonjs-to-es6.js",
    "release:watch": "webpack --progress --hide-modules --watch --config  ./build/webpack.release.min.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/allen-garvey/dithermark-vue-color"
  },
  "author": "Allen Garvey",
  "dependencies": {
    "lodash.throttle": "^4.1.1"
  },
  "devDependencies": {
    "css-loader": "^3.4.2",
    "replace-in-file": "^5.0.2",
    "vue": "^2.6.11",
    "vue-loader": "^15.9.1",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11"
  },
  "license": "MIT"
}

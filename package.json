{
  "name": "vue-color",
  "sideEffects": [
    "./src/components/*.vue",
    "./src/components/common/*.vue"
  ],
  "version": "2.4.6",
  "description": "Color of Vue Components",
  "keywords": [
    "color",
    "vuejs"
  ],
  "main": "dist/vue-color.min.js",
  "scripts": {
